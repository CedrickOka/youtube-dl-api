{
	"name" : "coka/youtube-dl-api",
	"type" : "project",
	"homepage" : "https://github.com/CedrickOka/youtube-dl-api",
	"license" : "MIT",
	"authors" : [{
			"name" : "Cedrick Oka",
			"email" : "okacedrick@gmail.com"
		}
	],
	"require" : {
		"php": "^7.1.3",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"coka/cors-bundle": "^1.2",
		"coka/rest-request-validator-bundle": "^1.1",
		"guzzlehttp/guzzle": "^6.3",
		"symfony/console": "4.4.*",
		"symfony/dotenv": "4.4.*",
		"symfony/flex": "^1.1",
		"symfony/framework-bundle": "4.4.*",
		"symfony/messenger": "4.4.*",
		"symfony/monolog-bundle": "^3.4",
		"symfony/yaml": "4.4.*"
	},
	"config" : {
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"replace" : {
		"paragonie/random_compat" : "2.*",
		"symfony/polyfill-ctype" : "*",
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php71" : "*",
		"symfony/polyfill-php70" : "*",
		"symfony/polyfill-php56" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"extra" : {
		"symfony" : {
			"allow-contrib" : false,
			"require" : "4.4.*"
		}
	}
}

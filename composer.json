{
	"name" : "veone/youtube-dl",
	"type" : "project",
	"license" : "proprietary",
	"require" : {
		"php": "^7.1.3",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"coka/api-bundle": "^3.0",
		"symfony/console": "4.2.*",
		"symfony/dotenv": "4.2.*",
		"symfony/flex": "^1.1",
		"symfony/framework-bundle": "4.2.*",
		"symfony/yaml": "4.2.*"
	},
	"config" : {
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"replace" : {
		"paragonie/random_compat" : "2.*",
		"symfony/polyfill-ctype" : "*",
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php71" : "*",
		"symfony/polyfill-php70" : "*",
		"symfony/polyfill-php56" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "*"
	},
	"extra" : {
		"symfony" : {
			"allow-contrib" : false,
			"require" : "4.2.*"
		}
	}
}
